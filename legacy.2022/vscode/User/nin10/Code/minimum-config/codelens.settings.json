{
    /*

    see 'settings.json' for colors

    */
    "errorLens.enabledDiagnosticLevels": [
        "error",
        "warning",
        "hint",
        "info"
    ],
    "errorLens.delay": 1300,
    "errorLens.exclude": [], //# regex array
    "errorLens.excludeBySource": [], // ex: ["eslint"]
    "errorLens.followCursor": "closestProblem",
    "errorLens.followCursorMore": 2,
    "errorLens.statusBarIconsEnabled": true,
    // ex: language specific regexs to ignore
    "[json][jsonc]": {
        "errorLens.exclude": [
            "duplicate.*key"
        ], //# regex
    }
}